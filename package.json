{
  "name": "@figureland/infinitykit",
  "version": "0.0.1",
  "main": "src/index.ts",
  "types": "src/index.ts",
  "type": "module",
  "description": "A modular Typescript toolkit for building minimal infinite canvas apps",
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "require": "./src/index.ts",
      "import": "./src/index.ts"
    }
  },
  "scripts": {
    "build": "bun run scripts/build.ts",
    "dev": "bun --watch scripts/dev.ts",
    "prepare": "husky",
    "lint": "eslint src/",
    "format": "prettier --write src/",
    "release": "bunx changelogen@latest --release",
    "size": "bunx size-limit"
  },
  "files": [
    "dist"
  ],
  "license": "MIT",
  "devDependencies": {
    "@commitlint/cli": "^19.2.1",
    "@commitlint/config-conventional": "^19.1.0",
    "@size-limit/preset-small-lib": "^11.1.2",
    "@types/bun": "^1.0.0",
    "bun-plugin-dts": "^0.2.1",
    "eslint": "^9.0.0",
    "husky": "^9.0.11",
    "prettier": "^3.2.5",
    "typescript": "^5.2.2",
    "typescript-eslint": "^7.6.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/figureland/infinitykit.git"
  },
  "author": "Figure Systems",
  "bugs": {
    "url": "https://github.com/figureland/infinitykit"
  },
  "homepage": "https://github.com/figureland/infinitykit#readme",
  "peerDependencies": {
    "@figureland/mathkit": ">=0.0.37",
    "@figureland/statekit": ">=0.0.115",
    "@figureland/typekit": ">=0.0.29",
    "superjson": "^2.2.1"
  },
  "peerDependenciesMeta": {
    "@figureland/mathkit": {
      "optional": false
    },
    "@figureland/statekit": {
      "optional": false
    },
    "@figureland/toolkit": {
      "optional": false
    },
    "@figureland/typekit": {
      "optional": false
    },
    "superjson": {
      "optional": true
    }
  },
  "dependencies": {
    "@figureland/mathkit": "^0.0.37",
    "@figureland/statekit": "0.0.116",
    "@figureland/toolkit": "0.0.71",
    "@figureland/typekit": "^0.0.29",
    "superjson": "^2.2.1"
  },
  "size-limit": [
    {
      "path": "dist/index.js"
    }
  ]
}
